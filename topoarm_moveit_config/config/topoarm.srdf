<?xml version="1.0" encoding="UTF-8"?>
<robot name="topoarm">
    <group name="arm">
        <joint name="joint1"/>
        <joint name="joint2"/>
        <joint name="joint3"/>
        <joint name="joint4"/>
        <joint name="joint5"/>
        <joint name="joint6"/>
        <joint name="end_effector_joint"/>
    </group>
    <group name="gripper">
        <joint name="gripper_left_joint"/>
        <passive_joint name="gripper_right_joint"/>
    </group>
    <group_state name="home" group="arm">
        <joint name="joint1" value="0.0" />
        <joint name="joint2" value="-1.10" />
        <joint name="joint3" value="1.87" />
        <joint name="joint4" value="0.0" />
        <joint name="joint5" value="0.98" />
        <joint name="joint6" value="0.0" />
    </group_state>
    <group_state name="close" group="gripper">
        <joint name="gripper_left_joint" value="-0.01"/>
    </group_state>
    <group_state name="open" group="gripper">
        <joint name="gripper_left_joint" value="0.01"/>
    </group_state>
    <end_effector name="end_effector_link" parent_link="end_effector_link" group="gripper"/>
    <disable_collisions link1="link1" link2="link2" reason="Adjacent"/>
    <disable_collisions link1="link2" link2="link3" reason="Adjacent"/>
    <disable_collisions link1="link3" link2="link4" reason="Adjacent"/>
    <disable_collisions link1="link4" link2="link5" reason="Adjacent"/>
    <disable_collisions link1="link5" link2="link6" reason="Adjacent"/>
    <disable_collisions link1="link6" link2="link7" reason="Adjacent"/>
    <disable_collisions link1="gripper_left_link" link2="gripper_right_link" reason="Never"/>
    <disable_collisions link1="gripper_left_link" link2="link4" reason="Never"/>
    <disable_collisions link1="gripper_left_link" link2="link5" reason="Never"/>
    <disable_collisions link1="gripper_left_link" link2="link6" reason="Never"/>
    <disable_collisions link1="gripper_left_link" link2="link7" reason="Adjacent"/>
    <disable_collisions link1="gripper_right_link" link2="link4" reason="Never"/>
    <disable_collisions link1="gripper_right_link" link2="link5" reason="Never"/>
    <disable_collisions link1="gripper_right_link" link2="link6" reason="Never"/>
    <disable_collisions link1="gripper_right_link" link2="link7" reason="Adjacent"/>
</robot>